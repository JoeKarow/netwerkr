{
	"configurations": [
		{
			"name": "Next.js: app:web",
			"type": "node-terminal",
			"request": "launch",
			"command": "pnpx turbo run dev",
			// "cwd": "${workspaceFolder:âœ¨ @joekarow/netwerkr}",
			"sourceMaps": true
		},
		{
			"name": "Attach to Process",
			"type": "node",
			"request": "attach",
			"processId": "${command:PickProcess}"
		},
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Launch Firefox Dev",
			"url": "http://localhost:3000",
