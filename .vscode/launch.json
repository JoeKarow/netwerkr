			"skipFiles": ["**/node_modules/**", "<node_internals>/**/*.js"],
			"showConsoleCallLocation": true,
			"suggestPathMappingWizard": true,
			"pathMappings": [
				{
					"url": "webpack://_n_e/packages",
					"path": "${workspaceFolder}/packages"
				}
			]
		}
