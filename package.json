{
  "name": "netwerkr",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "^1.2.14"
  },
  "engines": {
    "pnpm": ">=7.1.7",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.1.7"
}
